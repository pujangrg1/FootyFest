{
  "name": "soccer-tournament-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "node start-expo.js",
    "start:safe": "bash start-with-limit.sh",
    "start:tunnel": "node start-expo-tunnel.js",
    "start:lan": "node start-expo.js --lan",
    "android": "node start-expo.js --android",
    "ios": "node start-expo.js --ios",
    "web": "expo start --web",
    "build:web": "NODE_ENV=production expo export --platform web --output-dir web-build",
    "build:web:staging": "NODE_ENV=staging expo export --platform web --output-dir web-build",
    "serve:web": "npx serve web-build",
    "prebuild:web": "npm run clean:web",
    "clean:web": "rm -rf web-build"
  },
  "dependencies": {
    "@expo/metro-runtime": "^6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^2.0.1",
    "date-fns": "^3.0.0",
    "expo": "~54.0.0",
    "expo-asset": "~12.0.10",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-notifications": "~0.32.13",
    "expo-sharing": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "firebase": "^9.23.0",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.11.3",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.21.2",
    "react-redux": "^9.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "babel-preset-expo": "~54.0.0",
    "react-refresh": "^0.18.0"
  },
  "private": true
}
